<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须是对应接口的地址 -->
<mapper namespace="com.ysc.dao.CityMapper">
    <select id="selectCityByPId" resultType="com.ysc.model.City" parameterType="com.ysc.model.Province">
    select * from city where pId=#{id}
    </select>
</mapper>